{
    "name": "michaelr0/buildy-wp-gui",
    "description": "Buildy GUI for WP",
    "keywords": [
        "michaelr0",
        "buildy-wp-gui"
    ],
    "homepage": "https://github.com/michaelr0/buildy-wp-gui",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Michael Rook",
            "email": "michael@rook.net.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Michaelr0\\BuildyWpGui\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Michaelr0\\BuildyWpGui\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
